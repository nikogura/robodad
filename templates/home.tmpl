<html>
<head>
	<title> Robodad ArgusEye </title>
    	<link rel="stylesheet" href="/css/dark-hive/jquery-ui.min.css"/>
    	<link rel="stylesheet" href="/css/dark-hive/jquery.ui.theme.css"/>
    	<script type="text/javascript" src="/js/jquery-1.9.1.js"></script>
    	<script type="text/javascript" src="/js/jquery-ui-1.10.2.min.js"></script>
	

	<script type="text/javascript">
	
		$(document).ready(function() {
		    $('#main').addClass("ui-state-active");
		    $('#main').addClass("ui-corner-all");

		});

		function doIt(sound) {
			$.ajax({
				type: "GET",
				url: "/cgi-bin/arguseye",
				data: "sound="+sound,
				success: function(response) {
				},
				error: function(e) {

				}
			});

		}

	</script>
</head>
<body>
	<div id="main">
		<table>
			<thead>
			</thead>
			<tbody>
				<tr>
					<td>
						<input type="button" id="buzzer" value="Sound Buzzer" onclick="doIt('buzzer');"/>
					</td>
				</tr>
				<tr>
					<td>
						<input type="button" id="gotobed" value="Go To Bed" onclick="doIt('gotobed');" />
					</td>
				</tr>
				<tr>
					<td>
						<input type="button" id="liedown" value="Lie Down" onclick="doIt('liedown');"/>
					</td>
				</tr>
			
			</tbody>
		</table>
	</div>
</body>

</html>
